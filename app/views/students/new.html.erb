<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Student Form</title>
  </head>
  <body>
    <%= form_tag students_path do %>
    <!-- submits to index action -->
    <label>Student First Name/last_name:</label>
    <%= text_field_tag :first_name %>
    <%= text_field_tag :last_name %>
    <%= submit_tag 'Submit Student' %>
    <% end %>

    <label></label>
  </body>
</html>
